<BaseLayout title={pageTitle} metaTitle={metaTitle} metaDescription={metaDescription} norobots=true landingpage>
  <!-- Sticky Header -->
  <header class="bg-white sticky top-0 w-full z-50">
      <nav class="mx-auto flex max-w-7xl items-center justify-between p-6 lg:px-8">
        <a href="#" class="-m-1.5 p-1.5">
          <span class="sr-only">Galvix</span>
          <img class="h-8 w-auto" src="/images/galvix-logo.png" alt="Galvix Logo">
        </a>
        <div class="flex justify-end items-center">
            <div class="flex items-center gap-x-6">
                <a href="#cta" class="rounded-md bg-indigo-600 px-3.5 py-2.5 text-sm font-semibold text-white shadow-sm hover:bg-indigo-500 focus-visible:outline focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-indigo-600">Start a Free Trial</a>
            </div>
        </div>
      </nav>
  </header>

  <!-- Hero Section -->
  <HeroSectionForLPs 
    heading="Best Avalara Alternative" 
    subheading="Galvix is a modern sales tax service that provides exceptional customer support, with a transparent, all-inclusive pricing." 
    showClientLogos=true 
    formId="Avalara-alternative-lp-form" 
    formTitle="Start a Free Trial"
    formDescription="No credit card required."
    formButtonLabel="Sign up, It's Free"/>


---
import BaseLayout from '../../layouts/BaseLayout.astro';
import HeroSectionForLPs from '../../components/content-blocks/HeroSectionForLPs.astro';
import CustomerService from '../../components/content-blocks/CustomerService.astro';
import HowItWorks from '../../components/content-blocks/HowItWorks.astro';
import CTABlock from '../../components/content-blocks/CTABlock.astro';
import Testimonials from '../../components/content-blocks/Testimonials.astro';
const pageTitle = "Best Avalara Alternative for Sales Tax Management";
const metaTitle = "Best Avalara Alternative for Sales Tax Management";
const metaDescription = "Galvix is a modern sales tax service that provides exceptional customer support, with a transparent, all-inclusive pricing. Switch to Galvix and start focusing on your business, not paperwork.";
